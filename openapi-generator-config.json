{
  "generatorName": "typescript-fetch",
  "inputSpec": "https://chkok.kr/api-docs",
  "outputDir": "src/generate/api",
  "additionalProperties": {
    "withSeparateModelsAndApi": true,
    "apiPackage": "api",
    "modelPackage": "models",
    "supportsES6": true,
    "npmName": "my-api-client",
    "stringEnums": true,
    "enumNameSuffix": "Enum",
    "typescriptThreePlus": true
  },
  "globalProperties": {
    "skipFormModel": "false"
  },
  "systemProperties": {
    "javax.net.ssl.trustStore": "",
    "javax.net.ssl.trustStorePassword": ""
  }
}
